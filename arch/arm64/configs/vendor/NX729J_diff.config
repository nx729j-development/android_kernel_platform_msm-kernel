#add nubia kernel feature

CONFIG_NUBIAFULLCONFIG=y
CONFIG_NUBIA_HW_CONFIG_BY_GPIO=y
CONFIG_NUBIA_HW_VERSION=m
CONFIG_NUBIA_USB_CONFIG=y
CONFIG_NUBIA_VENDOR_HOOK=m
